<div class="mt-4">
    <div class="row">
         {{#each courses}}
        <div class="col-sm-6 col-lg-4">           
            <div class="card mt-4">
            <img src="{{this.img}}" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <p class="card-text">{{this.desc}}</p>
                <a href="#" class="btn btn-primary">Go in</a>
            </div>
            </div>          
        </div>  {{/each}}
    </div>
</div>


